{
  "name": "joshuaspence-phabricator",
  "version": "0.4.0",
  "author": "Joshua Spence",
  "license": "MIT",
  "summary": "Module to install and configure Phabricator, a suite of web-based software development collaboration tools.",
  "source": "https://github.com/joshuaspence/puppet-phabricator",
  "project_page": "https://github.com/joshuaspence/puppet-phabricator",
  "issues_url": "https://github.com/joshuaspence/puppet-phabricator/issues",
  "tags": [
    "arcanist",
    "libphutil",
    "phabricator"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/git",
      "version_requirement": "< 1.0.0"
    },
    {
      "name": "puppet/logrotate",
      "version_requirement": "2.x"
    },
    {
      "name": "puppet/nodejs",
      "version_requirement": "< 3.0.0"
    },
    {
      "name": "puppet/php",
      "version_requirement": "4.x"
    },
    {
      "name": "saz/ssh",
      "version_requirement": ">= 2.9.0 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.2.0 < 5.0.0"
    },
    {
      "name": "saz/sudo",
      "version_requirement": ">= 2.3.0 < 5.0.0"
    },
    {
      "name": "camptocamp/systemd",
      "version_requirement": ">= 0.3.0 < 1.0.0"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 1.0.0 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.9.0 < 6.0.0"
    }
  ]
}
